{
  "name": "@ng-select/ng-select",
  "version": "12.0.6",
  "description": "Angular ng-select - All in One UI Select, Multiselect and Autocomplete",
  "author": "@ng-select/ng-select",
  "license": "MIT",
  "repository": "ng-select/ng-select",
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "keywords": [
    "angular",
    "select",
    "ui-select",
    "dropdown",
    "multiselect",
    "autocomplete",
    "angular2"
  ],
  "exports": {
    "./scss/ant.design.theme": {
      "style": "./scss/ant.design.theme.scss"
    },
    "./scss/default.theme": {
      "style": "./scss/default.theme.scss"
    },
    "./scss/material.theme": {
      "style": "./scss/material.theme.scss"
    },
    "./themes/ant.design.theme.css": {
      "style": "./themes/ant.design.theme.css"
    },
    "./themes/default.theme.css": {
      "style": "./themes/default.theme.css"
    },
    "./themes/material.theme.css": {
      "style": "./themes/material.theme.css"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ng-select-ng-select.mjs",
      "esm": "./esm2022/ng-select-ng-select.mjs",
      "default": "./fesm2022/ng-select-ng-select.mjs"
    }
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0-rc.0",
    "@angular/core": "^17.0.0-rc.0",
    "@angular/forms": "^17.0.0-rc.0"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "module": "fesm2022/ng-select-ng-select.mjs",
  "typings": "index.d.ts",
  "sideEffects": false
}
